<% include ./partials/header %>

<% blogs.forEach(function(blog){ %>
    <div class="ui main container">
      <h2><%= blog.title %></h2>
      <img src="<%= blog.image %>" alt="Main blog image">
      <span>
        <%= blog.created %>
      </span>
      <p><%= blog.body %></p>
    </div>
<% }); %>

<% include ./partials/footer %>
